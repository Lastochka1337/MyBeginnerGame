        elif x == "a2":
            if o == "c3":
                if x2 == "b3":
                    os = 2
                    penup()
                    goto(-10,60)
                    pendown()
                    nolik()
        elif x == "a1":
            if o == "a3":
                if x2 == "c1":
                    os = 2
                    penup()
                    goto(-30,50)
                    pendown()
                    nolik()
                else:
                    os = 2
                    penup()
                    goto(-10,60)
                    pendown()
                    nolik()
            else:
                os = 2
                penup()
                goto(-10,60)
                pendown()                
                nolik()
    if o2 == "c3":
        if x == "c3":
            while o2 == "c3":
                print("На этом месте уже стоит крестик.")
                o2 = input("Введите куда хотите поставить нолик.")
        elif o == "c3":
            while o2 == "c3":
                print("На этом месте уже стоит нолик.")
                o2 = input("Введите куда хотите поставить нолик.")
        elif x2 == "c3":
            while o2 == "c3":
                print("На этом месте уже стоит крестик.")
                o2 = input("Введите куда хотите поставить нолик.")
        elif x == "b1":
            if o == "b3":
                if x2 == "b2":
                    os = 2
                    penup()
                    goto(160,20)
                    pendown()
                    nolik()
        else:
            os = 2
            penup()
            goto(110,55)
            pendown()
            nolik()

