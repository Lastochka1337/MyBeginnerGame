from turtle import *
speed(100)

# Функия которая рисует крестик
def krestik():
    color("blue")
    width(5)
    forward(70)
    right(180)
    forward(35)
    right(75)
    forward(35)
    right(180)
    forward(70)

# Функция которая рисует нолик
def nolik():
    width(5)
    color("red")
    circle(30)

# Функция которая рисует линию обозначающую победу для крестиков
def pobeda_dlya_X():
    color("blue")
    forward(450)

# Функция которая рисует линию обозначающую победу для ноликов
def pobeda_dlya_O():
    color("red")
    forward(450)

# Правила
print("Правила просты: 1 ряд 1 квадрат - a1, 1 ряд 2 квадрат - a2, 1 ряд 3 квадрат - a3, 2 ряд 1 квадрат - b1, 2 ряд 2 квадрат - b2, 2 ряд 3 квадрат - b3, 3 ряд 1 квадрат - c1, 3 ряд 2 квадрат - c2, 3 ряд 3 квадрат - c3. Первые ходят крестики")

# Таблица
width(5)
left(180)
forward(200)
left(180)
forward(400)
left(90)
forward(240)
left(90)
forward(400)
left(90)
forward(240)
left(90)
forward(133)
left(90)
forward(240)
right(90)
forward(133)
right(90)
forward(240)
left(90)
forward(134)
left(90)
forward(80)
left(90)
forward(400)
right(90)
forward(80)
right(90)
forward(400)

# Переменные для подчёта ходов.xs - крестики,os - нолики.
xs = 0
os = 0

#  1 ход Крестики
x = input("Введите куда хотите поставить креcтик.")

if x == "a1":
    xs+=1
    penup()
    goto(-115,230)
    right(135)
    pendown()
    krestik()
if x == "a2":
    xs+=1
    penup()
    goto(30,230)
    right(135)
    pendown()
    krestik()
if x == "a3":
    xs+=1
    penup()
    goto(140,230)
    right(135)
    pendown()
    krestik()

if x == "b1":
    xs+=1
    penup()
    goto(-100,100)
    left(180)
    forward(70)
    right(145)
    pendown()
    krestik()
if x == "b2":
    xs+=1
    penup()
    goto(30,150)
    right(135)
    pendown()
    krestik()
if x == "b3":
    xs+=1
    penup()
    goto(150,150)
    right(135)
    pendown()
    krestik()

if x == "c1":
    xs+=1
    penup()
    goto(-115,60)
    right(135)
    pendown()
    krestik()
if x == "c2":
    xs+=1
    penup()
    goto(30,60)
    right(135)
    pendown()
    krestik()
if x == "c3":
    xs+=1
    penup()
    goto(140,60)
    right(135)
    pendown()
    krestik()

while xs != 1:
    print("Вы ввели неправильные кoординаты. Попробуйте ещё раз.")
    x = input("Введите куда хотите поставить крестик.")
    if x == "a1":
        xs+=1
        penup()
        goto(-115,230)
        right(135)
        pendown()
        krestik()
    if x == "a2":
        xs+=1
        penup()
        goto(30,230)
        right(135)
        pendown()
        krestik()
    if x == "a3":
        xs+=1
        penup()
        goto(140,230)
        right(135)
        pendown()
        krestik()

    if x == "b1":
        xs+=1
        penup()
        goto(-100,100)
        left(180)
        forward(70)
        right(145)
        pendown()
        krestik()
    if x == "b2":
        xs+=1
        penup()
        goto(30,150)
        right(135)
        pendown()
        krestik()
    if x == "b3":
        xs+=1
        penup()
        goto(150,150)
        right(135)
        pendown()
        krestik()

    if x == "c1":
        xs+=1
        penup()
        goto(-115,60)
        right(135)
        pendown()
        krestik()
    if x == "c2":
        xs+=1
        penup()
        goto(30,60)
        right(135)
        pendown()
        krestik()
    if x == "c3":
        xs+=1
        penup()
        goto(140,60)
        right(135)
        pendown()
        krestik()

# 1 ход Нолики
o = input("Введите куда хотите поставить нолик.")

if o == "a1":
    if x == "a1":
        while o == "a1":
            print("На этом месте уже стоит крестик.")
            o = input("Введите куда хотите поставить нолик")
    elif x == "b1":
        os+=1
        penup()
        goto(-150,177)
        pendown()
        nolik()
    else:
        os+=1
        penup()
        goto(-115,227)
        pendown()
        nolik()
